void DispPhto(int nBitmapDot[])
{
  int j = 0x00, column, disp_data, page;
  int i = 0;
  int dz = 0;
  for (j = 0x00; j < 0x02; j++)
  {
    page = (j | 0xb0); //因为前面是固定的b，所以起始页为0
    LCD_WriteCommand(page);   //设置起始页0
    column = 0x10; //后4位为列地址的高位
    LCD_WriteCommand(column);
    for (i = 0; i < 131; i++)
    {
      dz++;
      disp_data = j * 8 + BitmapDot[i];
      LCD_WriteData(disp_data);
    }
  }
}

void ClearLCD() {
  int i, j, page;
  for (j = 0x00; j < 0x08; j++)
  {
    page = (j | 0xb0); //因为前面是固定的b，所以起始页为0
    LCD_WriteCommand(page);   //设置起始页0
    LCD_WriteCommand(0x10);
    for (i = 0; i < 128; i++)
    {
      LCD_WriteData(0x00);
    }
  }
}


void Lcd_Address(unsigned char page, unsigned char column) {
  //column=column-0x01;
  LCD_WriteCommand(0xb0 + page - 1);
  LCD_WriteCommand(0x10 + (column >> 4 & 0x0f));
  LCD_WriteCommand(column & 0x0f);
}


void Display_Graphic_8x16(unsigned char page, unsigned char column, unsigned char ascii) {
  int i, j, num;
  for (j = 0; j < 2; j++)
  {
    unsigned char Ascii_Str[8];
    Lcd_Address(page + j, column);
    for (num = 0; num < 8; num ++) {
      Ascii_Str[num] = pgm_read_byte_near(BitmapDot + (j * 8 + num + ascii * 16));
    }
    for (i = 0; i < 8; i++)
    {
      LCD_WriteData(Ascii_Str[i]);
      //dp++;
    }
  }
}

void Display_String(unsigned char x, unsigned char y, unsigned char len, char *dp) {
  unsigned char i;
  for (i = 0; i < len; i++) {
    Display_Graphic_8x16(x, y + i * 8, *dp - 0x20);
    dp++;
  }
}


